<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data/>
    <FrameLayout android:id="@+id/framePhotoopenove"
        android:layout_height="match_parent" android:layout_width="match_parent">
        <ImageView android:id="@+id/imageRectangle"
            android:layout_gravity="start"
            android:layout_height="match_parent"
            android:layout_width="match_parent"
            android:scaleType="fitXY"
            imageRes="@{@drawable/img_rectangle_812x375}"
            tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle_812x375"/>
        <LinearLayout android:id="@+id/linearRowplus"
            android:layout_gravity="top|start"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_10pxv"
            android:layout_width="match_parent"
            android:orientation="horizontal" style="@style/groupStyle_1">
            <androidx.appcompat.widget.Toolbar
                android:id="@+id/toolbarToolbar"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_225pxv"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_marginTop="@dimen/_45pxv"
                android:layout_weight="0.9" android:layout_width="0dp" app:contentInsetStart="0dp">
                <LinearLayout android:id="@+id/linearRowellipse"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageEllipse"
                        android:layout_height="@dimen/_28pxh"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_width="@dimen/_28pxh"
                        android:scaleType="centerCrop"
                        circular="@{true}"
                        imageRes="@{@drawable/img_ellipse_1}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_ellipse_1"/>
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearColumnangelopantazis"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_8pxh"
                        android:layout_weight="0.69"
                        android:layout_width="0dp" android:orientation="vertical">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtAngeloPantazis"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@string/lbl_rimsha_ali"
                            style="@style/txtRobotobold13_1" tools:text="@string/lbl_rimsha_ali"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtAngelopantazisOne"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_8pxh"
                            android:layout_marginTop="@dimen/_1pxv"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@string/lbl_rimshaali2"
                            style="@style/txtRobotoregular11" tools:text="@string/lbl_rimshaali2"/>
                    </LinearLayout>
                </LinearLayout>
            </androidx.appcompat.widget.Toolbar>
            <ImageView android:id="@+id/imagePlus"
                android:layout_height="@dimen/_10pxh"
                android:layout_marginBottom="@dimen/_234pxv"
                android:layout_marginEnd="@dimen/_24pxh"
                android:layout_marginStart="@dimen/_224pxh"
                android:layout_marginTop="@dimen/_54pxv"
                android:layout_width="@dimen/_10pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_plus"
                tools:ignore="ContentDescription" tools:src="@drawable/img_plus"/>
        </LinearLayout>
    </FrameLayout>
</layout>
