<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data/>
    <FrameLayout android:id="@+id/framePhotoopenove"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStylewhite_A700">
        <ImageView android:id="@+id/imageRectangle"
            android:layout_gravity="start"
            android:layout_height="match_parent"
            android:layout_width="match_parent"
            android:scaleType="fitXY"
            imageRes="@{@drawable/img_rectangle}"
            tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle"/>
        <LinearLayout android:id="@+id/linearRowplus"
            android:layout_gravity="top|start"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_10pxv"
            android:layout_width="match_parent"
            android:orientation="horizontal" style="@style/groupStyle_1">
            <LinearLayout android:id="@+id/linearRowellipse"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_225pxv"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_marginTop="@dimen/_46pxv"
                android:layout_weight="0.9" android:layout_width="0dp" android:orientation="horizontal">
                <ImageView android:id="@+id/imageEllipse"
                    android:layout_height="@dimen/_28pxh"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_width="@dimen/_28pxh"
                    android:scaleType="centerCrop" circular="@{true}"
                    imageRes="@{@drawable/img_ellipse}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_ellipse"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtRidzjcob"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_8pxh"
                    android:layout_marginTop="@dimen/_15pxv"
                    android:layout_weight="0.7"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@string/lbl_fayaz_seeu"
                    style="@style/txtRobotoregular11" tools:text="@string/lbl_fayaz_seeu"/>
            </LinearLayout>
            <ImageView android:id="@+id/imagePlus"
                android:layout_height="@dimen/_10pxh"
                android:layout_marginBottom="@dimen/_234pxv"
                android:layout_marginEnd="@dimen/_24pxh"
                android:layout_marginStart="@dimen/_222pxh"
                android:layout_marginTop="@dimen/_54pxv"
                android:layout_width="@dimen/_10pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_plus"
                tools:ignore="ContentDescription" tools:src="@drawable/img_plus"/>
        </LinearLayout>
    </FrameLayout>
</layout>
